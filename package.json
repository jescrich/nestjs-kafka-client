{
  "name": "@jescrich/nestjs-kafka-client",
  "version": "1.0.0",
  "description": "NestJS Kafka Client and Consumer",
  "license": "MIT",
  "author": "Jos√© Escrich",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": false,
  "files": [
    "./dist/**"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "nest build",
    "clean": "del-cli -f \"./dist\" && del-cli -f \"./*.tsbuildinfo\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config jest.config.cjs --no-watchman",
    "test:watch": "jest --watch --config jest.config.cjs --no-watchman",
    "test:cov": "jest --coverage --config jest.config.cjs --no-watchman",
    "test:kafka": "jest --config jest.config.cjs test/kafka/ test/consumer/ --no-watchman"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/microservices": "^11.0.1",
    "@nestjs/terminus": "^10.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@nestjs/microservices": "^11.1.6",
    "@nestjs/terminus": "^11.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "kafkajs": "^2.2.4",
    "lodash": "^4.17.15",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.7.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/testing": "^11.1.6",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.17",
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "del-cli": "^6.0.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.0.5",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "_moduleAliases": {
    "@this/*": "*"
  }
}
